Cell["\<\
Calculate the total time to collect the data. Assume  sensor i has Data[[i]] \
in bits.\
\>", "Subsection",
 CellChangeTimes->{{3.653904966026248*^9, 3.6539049934628363`*^9}, {
  3.653905394273151*^9, 3.653905403908063*^9}, {3.653912959291893*^9, 
  3.653912983309017*^9}, {3.6539134188115997`*^9, 3.653913421588181*^9}, {
  3.6539416227784758`*^9, 3.6539416232956676`*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"maxVelmps", " ", "=", " ", "1"}], ";"}], " ", 
  RowBox[{"(*", 
   RowBox[{"robot", " ", "max", " ", "velocity", " ", "in", " ", 
    RowBox[{"meters", "/", "s"}]}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Bts", " ", "=", " ", "10"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"DataInEachSensorBits", "[", "Bts_", "]"}], " ", ":=", " ", 
   RowBox[{"ConstantArray", "[", 
    RowBox[{"Bts", ",", 
     RowBox[{"n", "-", "1"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"distToCommEachNode", "=", 
   RowBox[{"Table", "[", " ", 
    RowBox[{
     RowBox[{"EuclideanDistance", "[", 
      RowBox[{
       RowBox[{"p", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
       ",", 
       RowBox[{"p", "\[LeftDoubleBracket]", "j", "\[RightDoubleBracket]"}]}], 
      "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", " ", "2", ",", " ", 
       RowBox[{"Length", "[", "p", "]"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"distToVisitEachNode", "=", 
    RowBox[{"Table", "[", " ", 
     RowBox[{
      RowBox[{"EuclideanDistance", "[", 
       RowBox[{
        RowBox[{"p", "\[LeftDoubleBracket]", 
         RowBox[{"route", "\[LeftDoubleBracket]", 
          RowBox[{"j", "-", "1"}], "\[RightDoubleBracket]"}], 
         "\[RightDoubleBracket]"}], ",", 
        RowBox[{"p", "\[LeftDoubleBracket]", 
         RowBox[{
         "route", "\[LeftDoubleBracket]", "j", "\[RightDoubleBracket]"}], 
         "\[RightDoubleBracket]"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"j", ",", " ", "2", ",", " ", 
        RowBox[{"Length", "[", "route", "]"}]}], "}"}]}], "]"}]}], ";"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"time", " ", "is", " ", "distance", "*", "velocity"}], " ", "+", 
    " ", 
    RowBox[{"sum", 
