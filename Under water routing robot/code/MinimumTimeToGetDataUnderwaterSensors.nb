Cell[BoxData[{
 RowBox[{
  RowBox[{"distFromMean", "=", 
   RowBox[{"Table", "[", " ", 
    RowBox[{
     RowBox[{"EuclideanDistance", "[", 
      RowBox[{
       RowBox[{"Mean", "[", "p", "]"}], ",", 
       RowBox[{"p", "\[LeftDoubleBracket]", "j", "\[RightDoubleBracket]"}]}], 
      "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", " ", "2", ",", " ", 
       RowBox[{"Length", "[", "p", "]"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
