Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"maxVelmps", " ", "=", " ", "1"}], ";"}], " ", 
  RowBox[{"(*", 
   RowBox[{"robot", " ", "max", " ", "velocity", " ", "in", " ", 
    RowBox[{"meters", "/", "s"}]}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Bts", " ", "=", " ", "10"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"DataInEachSensorBits", "[", "Bts_", "]"}], " ", ":=", " ", 
   RowBox[{"ConstantArray", "[", 
    RowBox[{"Bts", ",", 
     RowBox[{"n", "-", "1"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"distToCommEachNode", "=", 
   RowBox[{"Table", "[", " ", 
    RowBox[{
     RowBox[{"EuclideanDistance", "[", 
      RowBox[{
       RowBox[{"p", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], 
       ",", 
       RowBox[{"p", "\[LeftDoubleBracket]", "j", "\[RightDoubleBracket]"}]}], 
      "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", " ", "2", ",", " ", 
       RowBox[{"Length", "[", "p", "]"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"distToVisitEachNode", "=", 
    RowBox[{"Table", "[", " ", 
     RowBox[{
      RowBox[{"EuclideanDistance", "[", 
       RowBox[{
        RowBox[{"p", "\[LeftDoubleBracket]", 
         RowBox[{"route", "\[LeftDoubleBracket]", 
          RowBox[{"j", "-", "1"}], "\[RightDoubleBracket]"}], 
         "\[RightDoubleBracket]"}], ",", 
        RowBox[{"p", "\[LeftDoubleBracket]", 
         RowBox[{
         "route", "\[LeftDoubleBracket]", "j", "\[RightDoubleBracket]"}], 
         "\[RightDoubleBracket]"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"j", ",", " ", "2", ",", " ", 
        RowBox[{"Length", "[", "route", "]"}]}], "}"}]}], "]"}]}], ";"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"time", " ", "is", " ", "distance", "*", "velocity"}], " ", "+", 
    " ", 
    RowBox[{"sum", 
     RowBox[{"(", 
      RowBox[{"bits", "*", 
       RowBox[{"communicate", "[", "distance", "]"}]}], ")"}]}]}], "   ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"totalTimeVisitEachNode", "[", "edgeLen_", "]"}], ":=", " ", 
   RowBox[{
    RowBox[{"edgeLen", "*", 
     RowBox[{
      RowBox[{"Total", "[", "distToVisitEachNode", "]"}], "/", 
      "maxVelmps"}]}], " ", "+", " ", 
    RowBox[{
     RowBox[{"Total", "[", 
      RowBox[{"DataInEachSensorBits", "[", "Bts", "]"}], "]"}], "/", 
     RowBox[{"bitsPerSecMagneticInduc", "[", "0", "]"}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"totalTimeCommEachNode", "[", "edgeLen_", "]"}], ":=", "  ", 
   RowBox[{"+", " ", 
    RowBox[{"Total", "[", 
     RowBox[{
      RowBox[{"DataInEachSensorBits", "[", "Bts", "]"}], "/", 
