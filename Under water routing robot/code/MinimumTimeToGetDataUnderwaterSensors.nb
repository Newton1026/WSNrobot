  RowBox[{"distMinStartAndMean", " ", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"Min", "[", "#", "]"}], "&"}], "/@", 
    RowBox[{"Transpose", "[", 
     RowBox[{"{", 
      RowBox[{"distToCommEachNode", ",", "distFromMean"}], "}"}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
